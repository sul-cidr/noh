  "captions": [
    {
      "startTime": 235,
      "endTime": 239,
      "translation": "[WAKIZURE] To begin with,",
      "transcription": "[WAKIZURE] Somo somo kore wa,"
    },
    {
      "startTime": 239,
      "endTime": 244,
      "translation": "I am in the service of the Emperor Ichijo.",
      "transcription": "Ichijō-no-In ni tsukae tatematsuru."
    },
    {
      "startTime": 244,
      "endTime": 252,
      "translation": "I am known as Tachibana no Michinari.",
      "transcription": "Tachibana-no-Michinari to wa waga koto nari."
    },
    {
      "startTime": 252,
      "endTime": 262,
      "translation": "It is so happens that his Lordship",
      "transcription": "Sate mo mikado kon'ya kidoku no go reimu, mashimashite."
    },
    {
      "startTime": 262,
      "endTime": 268,
      "translation": "has had a portentous dream oracle.",
      "transcription": "Sanjō-no-Kokaji Munechika ni."
    },
    {
      "startTime": 268,
      "endTime": 274,
      "translation": "The Sanjo swordsmith Munechika",
      "transcription": "Mi-tsurugi wo utaserarubeki to no"
    },
    {
      "startTime": 274,
      "endTime": 283,
      "translation": "must be ordered to forge a blade.",
      "transcription": "senji tadaima narikudarite sōrō hodo ni."
    },
    {
      "startTime": 283,
      "endTime": 290,
      "translation": "I am on my way now to tell Munechika",
      "transcription": "Kono yoshi wo Munechika ni mōshi-tsukebaya to"
    },
    {
      "startTime": 290,
      "endTime": 294,
      "translation": "of this matter.",
      "transcription": "zonji sōrō."
    }
  ],
